  "vim.insertModeKeyBindings": [
    {
      "before": ["f", "f"],
      "commands": [
        {
          "command": "selectby.regex",
          "args": ["insideTripleQuotes"]
        },
        "prettier-sql-vscode.format-selection",
        {
          "command": "editor.action.insertSnippet",
